{
  "name": "angular-starter-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "start-dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "start-prod": "cross-env NODE_ENV=production nodemon src/server.js",
    "start-test": "cross-env NODE_ENV=test nodemon src/server.js",
    "lint": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "webpack",
    "build-dev": "webpack --mode development --config webpack.config.js",
    "build-prod": "webpack --mode production --config webpack.config.js",
    "serve": "node dist/server.bundle.js",
    "test:single": "jest --testPathPattern",
    "test-db-adapter-factory": "npm run test:single -- src/core/database/__tests__/unit/db-adapter-factory.test.js",
    "test-mock-adapter": "npm run test:single -- src/core/database/__tests__/unit/mock-adapter.test.js",
    "test-mongodb-adapter": "npm run test:single -- src/core/database/__tests__/unit/mongodb-adapter.test.js",
    "test-mysql-adapter": "npm run test:single -- src/core/database/__tests__/unit/mysql-adapter.test.js",
    "test-postgres-adapter": "npm run test:single -- src/core/database/__tests__/unit/postgres-adapter.test.js",
    "test-db-connection-manager": "npm run test:single -- src/core/database/__tests__/unit/db-connection-manager.test.js",
    "test-db-connection-service": "npm run test:single -- src/core/database/__tests__/unit/db-connection-service.test.js",
    "test-continent-controller": "npm run test:single -- src/features/continent/__tests__/unit/continent-controller.test.js",
    "test-continent-model": "npm run test:single -- src/features/continent/__tests__/unit/continent-model.test.js",
    "test-continent-repository-mock": "npm run test:single -- src/features/continent/__tests__/unit/continent-repository-mock.test.js",
    "test-continent-repository-mysql": "npm run test:single -- src/features/continent/__tests__/unit/continent-repository-mysql.test.js",
    "test-continent-repository-postgresql": "npm run test:single -- src/features/continent/__tests__/unit/continent-repository-postgresql.test.js",
    "test-continent-repository": "npm run test:single -- src/features/continent/__tests__/unit/continent-repository.test.js",
    "test-continent-route": "npm run test:single -- __tests__/integration/features/continent-route.test.js",
    "test-continent-service": "npm run test:single -- src/features/continent/__tests__/unit/continent-service.test.js",
    "test-index-routes": "npm run test:single -- __tests__/integration/index-routes.test.js",
    "test-config": "jest --testPathPattern=src/core/config/__tests__/unit/config.test.js",
    "test-database-query": "jest --testPathPattern=src/core/database/__tests__/unit/database-query.test.js",
    "test-mock-data-loader": "jest --testPathPattern=src/core/database/__tests__/unit/mock-data-loader.test.js",
    "test-response-handler": "jest --testPathPattern=src/infrastructure/logger/__tests__/unit/response-handler.test.js",
    "test-query-utils": "jest --testPathPattern=src/shared/utils/__tests__/unit/query-utils.test.js",
    "test-request-logger": "jest --testPathPattern=src/infrastructure/logger/__tests__/unit/request-logger.test.js",
    "test-app": "jest --testPathPattern=__tests__/integration/app.test.js",
    "test-server": "jest --testPathPattern=__tests__/integration/server.test.js",
    "generate-structure": "node tools/scripts/generate-structure.js",
    "generate-results": "artillery run load-test.yml --output ./data/results/load-test-result.json && artillery report --output ./data/results/load-test-report.html ./data/results/load-test-result.json",
    "create-database": "node tools/database-scripts/create-database.js",
    "create-domains": "node tools/database-scripts/create-domains.js",
    "restore": "npm run create-database && npm run create-domains",
    "create-database-prod": "node dist/create-database.bundle.js",
    "create-domains-prod": "node dist/create-domains.bundle.js",
    "restore-prod": "npm run create-database-prod && npm run create-domains-prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "compression": "1.7.5",
    "copy-webpack-plugin": "12.0.2",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.4.7",
    "express": "4.21.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.11.0",
    "mysql2": "3.11.5",
    "nodemon": "3.1.7",
    "path": "0.12.7",
    "pg": "8.13.1",
    "pg-hstore": "2.3.4",
    "pg-promise": "11.10.2",
    "sequelize": "6.37.5",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "webpack-node-externals": "3.0.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@eslint/js": "9.16.0",
    "babel-loader": "9.2.1",
    "eslint": "9.16.0",
    "globals": "15.13.0",
    "jest": "29.7.0",
    "jsdoc": "4.0.4",
    "null-loader": "4.0.1",
    "supertest": "7.0.0",
    "webpack": "5.97.0",
    "webpack-cli": "5.1.4"
  }
}
